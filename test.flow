buildOp(op: string, e1: Expr, e2: Expr) -> Expr;
buildOp(op, e1, e2) {
	if(op=="+") {
		Sum(e1, e2);
	} else if(op=="-") {
		Sub(e1, e2);
	} else if(op=="*") {
		Mul(e1, e2);
	} else {
		Div(e1, e2);
 	}
}